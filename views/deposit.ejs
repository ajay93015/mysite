<!DOCTYPE html>
<html>
  <head>
    <title>Receipt</title>
    <script>
      window.onload = function() {
        document.getElementById("date").innerHTML = prompt("Enter Date (DD-MM-YYYY)");
        document.getElementById("time").innerHTML = prompt("Enter Time HH:MM:SS");
        document.getElementById("koCode").innerHTML = prompt("Enter KIOSK ID",);
        document.getElementById("ssaLocation").innerHTML = (document.getElementById("koCode").innerHTML !== "K0201508") ? "UBARA" : "DHAWAIYA";
        document.getElementById("customerName").innerHTML = prompt("Enter Customer Name",);
        document.getElementById("terminalId").innerHTML = prompt("Enter Terminal ID");
        document.getElementById("accountNumber").innerHTML = prompt("Enter Account Number","XXXXXXXXXXXX");
        document.getElementById("stan").innerHTML = prompt("Enter STAN", "251877");
        document.getElementById("rrn").innerHTML = prompt("Enter RRN");
        document.getElementById("transactionAmount").innerHTML = prompt("Enter Amount");
      };
    </script>
  </head>
  <body>
    <table align="center">
      <tbody>
        <tr>
          <td>
            <img style="padding: 0px !important; max-height: 55px;" src="https://bcportal.pnbfikiosk.co.in/kiosk/images/bank_log_main.jpg">
          </td>
        </tr>
      </tbody>
    </table>
    <div style="padding-bottom: 15px;" align="center" class="col-xs-12 col-sm-12">
      <label><b>Third Party Deposit</b></label>
    </div>
    <div align="center">
      <table border="1">
        <tbody>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Date</label></td><td id="date" style="text-align: right; padding-right: 5px; font-weight: 600;">-</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Time</label></td><td id="time" style="text-align: right; padding-right: 5px; font-weight: 600;">16:42:23</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>KO Name</label></td><td style="text-align: right; padding-right: 5px; font-weight: 600;">Prabha Vishwkarma</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>KO Code</label></td><td id="koCode" style="text-align: right; padding-right: 5px; font-weight: 600;">K0201508</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>SSA Location</label></td><td id="ssaLocation" style="text-align: right; padding-right: 5px; font-weight: 600;">DHAWAIYA</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Customer Name</label></td><td id="customerName" style="text-align: right; padding-right: 5px; font-weight: 600;">SHATRU DHAN PRASAD DUBEY S/O SAMPATLAL DUBEY</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Terminal ID</label></td><td id="terminalId" style="text-align: right; padding-right: 5px; font-weight: 600;">K0201508</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Account Number</label></td><td id="accountNumber" style="text-align: right; padding-right: 5px; font-weight: 600;">XXXXXXXXXXXX3511</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>STAN</label></td><td id="stan" style="text-align: right; padding-right: 5px; font-weight: 600;">251877</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>RRN</label></td><td id="rrn" style="text-align: right; padding-right: 5px; font-weight: 600;">504916251877</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Mode of Transaction</label></td><td style="text-align: right; padding-right: 5px; font-weight: 600;">KIOSK</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Transaction Amount</label></td><td id="transactionAmount" style="text-align: right; padding-right: 5px; font-weight: 600;">Rs. 12200.00</td></tr>
          <tr><td style="text-align: left; padding-left: 5px;"><label>Transaction Status</label></td><td style="text-align: right; padding-right: 5px; font-weight: 600;">SUCCESS(00)</td></tr>
        </tbody>
      </table>
    </div>
  </body>
</html>
